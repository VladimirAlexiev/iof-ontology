    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/GLNCodingSystem -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/GLNCodingSystem">
        <rdfs:subClassOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/CodingSystem"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/CodingSystem"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/prescribes"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/CodeIdentifier"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/designates"/>
                                        <owl:someValuesFrom>
                                            <owl:Class>
                                                <owl:unionOf rdf:parseType="Collection">
                                                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/BusinessOrganization"/>
                                                    <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/GeospatialLocation"/>
                                                </owl:unionOf>
                                            </owl:Class>
                                        </owl:someValuesFrom>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:label>global location number coding system</rdfs:label>
        <skos:altLabel>GLN coding system</skos:altLabel>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</iof-av:isPrimitive>
        <iof-av:logicAxiom>
            <rdf:Description>
                <iof-av:firstOrderLogicAxiom>GLNCodingSystem(x) →InformationContent Entity(x) ∧ ∃y(CodeIdentifier (y) ∧ prescribes (x,y)) ∧ ∃b((BusinessOrganization (b) ∨ GeospatialLocation(b)) ∧ designates (y,b))</iof-av:firstOrderLogicAxiom>
                <iof-av:semiFormalNaturalLanguageAxiom>GLN Coding System x implies x is an Information Content Entity and x prescribes some Code Identifier y that designates a Business Organization b or a Geospatial Location g</iof-av:semiFormalNaturalLanguageAxiom>
            </rdf:Description>
        </iof-av:logicAxiom>
        <iof-av:naturalLanguageDefinition>coding system that prescribes a GLN (global location number)</iof-av:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/GainOfRole -->

