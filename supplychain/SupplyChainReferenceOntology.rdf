        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</iof-av:isPrimitive>
        <iof-av:naturalLanguageDefinition>planned process in which the position (i.e. the orientation, alignment, or arrangement) of some material entity is changed with respect to some frame of reference</iof-av:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/PostalAddress -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/PostalAddress">
