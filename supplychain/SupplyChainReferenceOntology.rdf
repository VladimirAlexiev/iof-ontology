    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/PortionOfMaterial -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/PortionOfMaterial">
