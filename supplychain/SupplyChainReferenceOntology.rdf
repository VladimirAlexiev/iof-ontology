    <!-- https://purl.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/firstOrderLogicAxiom -->

    <owl:AnnotationProperty rdf:about="https://purl.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/firstOrderLogicAxiom"/>
    


    <!-- https://purl.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive -->

    <owl:AnnotationProperty rdf:about="https://purl.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive"/>
    


    <!-- https://purl.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/logicAxiom -->

    <owl:AnnotationProperty rdf:about="https://purl.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/logicAxiom"/>
    


    <!-- https://purl.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/primitiveRationale -->

    <owl:AnnotationProperty rdf:about="https://purl.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/primitiveRationale"/>
    


    <!-- https://purl.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/semiFormalNaturalLanguageAxiom -->

    <owl:AnnotationProperty rdf:about="https://purl.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/semiFormalNaturalLanguageAxiom"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/agentIn -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/agentIn">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000056"/>
        <owl:inverseOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/hasAgent"/>
        <rdfs:domain rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/Agent"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <rdfs:label>agent in</rdfs:label>
        <skos:altLabel>performer in</skos:altLabel>
        <iof-av:naturalLanguageDefinition>relation that holds between an agent and a process where the agent participates in the process and materially causes the occurrence of the process (plays a causal role).</iof-av:naturalLanguageDefinition>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/buyerIn -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/buyerIn">
        <rdfs:subPropertyOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/agentIn"/>
        <rdfs:domain rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/Agent"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <rdfs:label>buyer in</rdfs:label>
        <iof-av:naturalLanguageDefinition>relation that holds between an agent and a planned process in which the agent participates in the process as the buyer of a product or service</iof-av:naturalLanguageDefinition>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/capabilityOf -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/capabilityOf">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000197"/>
        <owl:inverseOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/hasCapability"/>
        <rdfs:label>capability of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/endsAt -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/endsAt">
        <rdfs:subPropertyOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/occursAt"/>
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000006"/>
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000029"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label>ends at</rdfs:label>
        <iof-av:semiFormalNaturalLanguageDefinition>An instance level relation that holds between instances of Processual Entities and Spatial Regions or Site whevever some spatial region or site is the location in which the process ends.</iof-av:semiFormalNaturalLanguageDefinition>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/hasAgent -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/hasAgent">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000057"/>
        <rdfs:label>has agent</rdfs:label>
        <iof-av:naturalLanguageDefinition>relation that holds between a process and agent when the agent participates in the process and plays a causal role</iof-av:naturalLanguageDefinition>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/hasCapability -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/hasCapability">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000196"/>
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/BFO_0000004"/>
        <rdfs:range rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/Capability"/>
        <rdfs:label>has capability</rdfs:label>
        <iof-av:elucidation>a relation between an independent continuant (the bearer) and a capability, in which the capability specifically depends on the bearer for its existence</iof-av:elucidation>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/hasParticipantAsBuyer -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/hasParticipantAsBuyer">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000057"/>
        <rdfs:label>has participant as buyer</rdfs:label>
        <iof-av:naturalLanguageDefinition>relation that holds between a process and an agent in which the agent participates in the process as the buyer of some product or service</iof-av:naturalLanguageDefinition>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/hasParticipantAsSeller -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/hasParticipantAsSeller">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000057"/>
        <rdfs:label>has participant as seller</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/hasParticipantAsTradeItem -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/hasParticipantAsTradeItem">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000057"/>
        <owl:inverseOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/participatesInAsTradeItem"/>
        <rdfs:label>has participant as trade item</rdfs:label>
        <iof-av:naturalLanguageDefinition>relation that holds between a buying process or a supplying process and a product in which the product participates in the process as the subject of trade</iof-av:naturalLanguageDefinition>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/hasSkill -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/hasSkill">
        <rdfs:subPropertyOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/hasCapability"/>
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/BFO_0000004"/>
        <rdfs:range rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/Skill"/>
        <rdfs:label>has skill</rdfs:label>
        <iof-av:elucidation>a relation between an independent continuant (the bearer) and a skill, in which the skill specifically depends on the bearer for its existence</iof-av:elucidation>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/holds -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/holds">
        <rdfs:domain rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/Agent"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000004"/>
        <rdfs:label>holds</rdfs:label>
        <skos:example>Amazon &apos;holds&apos; a shipment when it is out for delivery but not delivered yet.</skos:example>
        <iof-av:naturalLanguageDefinition>(x) holds (y) implies that (x) is an Agent and (y) is an independent continuant and (x) physically keeps or carries (y)</iof-av:naturalLanguageDefinition>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/initiatesRelationship -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/initiatesRelationship">
        <owl:inverseOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/relationshipInitiatedBy"/>
        <rdfs:domain rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/Event"/>
        <rdfs:label>initiates relationship</rdfs:label>
        <iof-av:naturalLanguageDefinition>a relation between an instance of an event and an instance of a relational specifically dependent continuant where the event establishes (originates) the relational specifically dependent continuant</iof-av:naturalLanguageDefinition>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/isSiteOf -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/isSiteOf">
        <owl:inverseOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/occursAt"/>
        <rdfs:label>is site of</rdfs:label>
        <iof-av:semiFormalNaturalLanguageDefinition>x &apos;is site of&apos; y iff x is an instance of Site and y is an instance of Process, such that y occurs at x.</iof-av:semiFormalNaturalLanguageDefinition>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/occursAt -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/occursAt">
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000006"/>
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000029"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label>occurs at</rdfs:label>
        <iof-av:naturalLanguageDefinition>An instance level relation that holds between instances of Processual Entities and Spatial Regions or Site whevever some spatial region or site is the location in which the process unfolds.</iof-av:naturalLanguageDefinition>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/occursOn -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/occursOn">
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000008"/>
        <rdfs:label>occurs on</rdfs:label>
        <iof-av:naturalLanguageDefinition>relation which holds between some processual entity and some temporal region whenever the duration of the processual entity is contained by the temporal region</iof-av:naturalLanguageDefinition>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ownedBy -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ownedBy">
        <owl:inverseOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/owns"/>
        <rdfs:label>owned by</rdfs:label>
        <iof-av:naturalLanguageDefinition>(a) owned by (b) implies that (a) is an Agent and (b) is an &apos;independant continuant&apos; and (a) holds (b) as legal property</iof-av:naturalLanguageDefinition>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/owns -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/owns">
        <rdfs:label>owns</rdfs:label>
        <iof-av:naturalLanguageDefinition>(a) owns (b) implies that  (a) is an Agent and (b) is an &apos;independant continuant&apos; and (a) holds (b) as legal property</iof-av:naturalLanguageDefinition>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/participatesInAsTradeItem -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/participatesInAsTradeItem">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000056"/>
        <rdfs:domain rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/MaterialProduct"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/BuyingBusinessProcess"/>
                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/SupplyingBusinessProcess"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label>participates in as trade item</rdfs:label>
        <iof-av:explanatoryNote>A trade item is any type of good or service that can be considered for purchase. However, in this relationship, only material entites can be at the domain.</iof-av:explanatoryNote>
        <iof-av:naturalLanguageDefinition>relation that holds between a product and a buying process or a supplying process in which the product participates in the process as the subject of trade</iof-av:naturalLanguageDefinition>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/relationshipInitiatedBy -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/relationshipInitiatedBy">
        <rdfs:label>relationship initiated by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/sellerIn -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/sellerIn">
        <rdfs:subPropertyOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/agentIn"/>
        <rdfs:domain rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/Agent"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <rdfs:label>seller in</rdfs:label>
        <iof-av:naturalLanguageDefinition>relation that holds between an agent and a planned process in which the agent participates in the process as the eller of a product or service</iof-av:naturalLanguageDefinition>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/skillOf -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/skillOf">
        <rdfs:subPropertyOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/capabilityOf"/>
        <rdfs:label>skill of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/startsAt -->

    <owl:ObjectProperty rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/startsAt">
        <rdfs:subPropertyOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/occursAt"/>
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000006"/>
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000029"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label>starts at</rdfs:label>
        <iof-av:semiFormalNaturalLanguageDefinition>An instance level relation that holds between instances of Processual Entities and Spatial Regions or Site whevever some spatial region or site is the location in which the process starts.</iof-av:semiFormalNaturalLanguageDefinition>
    </owl:ObjectProperty>
    


