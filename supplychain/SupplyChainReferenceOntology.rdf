    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/Lot -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/Lot">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000040"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/hasRole"/>
                        <owl:someValuesFrom rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/LotRole"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/TraceableResourceUnit"/>
        <rdfs:label>lot</rdfs:label>
        <iof-av:adaptedFrom>APICS</iof-av:adaptedFrom>
        <iof-av:firstOrderLogicDefinition>Lot(x) ↔ MaterialEntity(x) ∧ ∃r (LotRole (r) ∧ hasRole (x,r))</iof-av:firstOrderLogicDefinition>
        <iof-av:naturalLanguageDefinition>quantity of material entities produced together and sharing the same production history and specifications</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>Lot is a Material Entity that &apos;has role&apos; some Lot Role</iof-av:semiFormalNaturalLanguageDefinition>
