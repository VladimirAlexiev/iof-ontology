    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/GeospatialLocation -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/GeospatialLocation">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/GeospatialSite"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/hasRole"/>
