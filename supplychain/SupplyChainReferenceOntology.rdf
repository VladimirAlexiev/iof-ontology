    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/LoadRole -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/LoadRole">
