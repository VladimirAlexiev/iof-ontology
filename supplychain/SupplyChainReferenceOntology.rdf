        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</iof-av:isPrimitive>
        <iof-av:naturalLanguageDefinition>organism that is multicellular, eukaryotic, heterotrophic, and capable of motility for part of its life</iof-av:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ApprasialProcess -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ApprasialProcess">
