    <owl:Ontology rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/">
        <owl:imports rdf:resource="https://purl.industrialontologies.org/ontology/core/20220506/Core/"/>
        <dcterms:abstract>Supply Chain Reference Ontology (SCRO) aims to represent the generic constructs (including classes and properties) related to the domain of supply chain and logistics. SCRO uses BFO as the top-level ontology and IOF Core as the mid-level ontology.</dcterms:abstract>
        <dcterms:contributor>Boonserm Kulvatanyou, NIST</dcterms:contributor>
        <dcterms:contributor>Chris Will, National Center for Ontological Research (NCOR)</dcterms:contributor>
        <dcterms:contributor>Dusan Sormaz, Ohio University</dcterms:contributor>
        <dcterms:contributor>Evan Wallace, NIST</dcterms:contributor>
        <dcterms:contributor>IOF Supply Chain Working Group</dcterms:contributor>
        <dcterms:contributor>Jim Logan, No Magic Inc.</dcterms:contributor>
        <dcterms:creator>Farhad Ameri, Texas State University</dcterms:creator>
        <dcterms:license rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://opensource.org/licenses/MIT</dcterms:license>
        <dcterms:publisher>Industrial Ontology Foundry</dcterms:publisher>
        <dcterms:references rdf:resource="http://purl.org/dc/terms/"/>
        <dcterms:references rdf:resource="http://www.w3.org/2004/02/skos/core#"/>
        <dcterms:title>IOF Supply Chain Reference Ontology (SCRO)</dcterms:title>
        <rdfs:label>Supply Chain Reference Ontology</rdfs:label>
        <iof-av:acronym>SCRO</iof-av:acronym>
        <iof-av:copyright>Copyright (c) 2022 Open Applications Group</iof-av:copyright>
    </owl:Ontology>
    


