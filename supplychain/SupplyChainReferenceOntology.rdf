        <rdfs:subClassOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/SupplyChainSystem"/>
        <rdfs:label>manufacturing supply chain system</rdfs:label>
        <iof-av:firstOrderLogicDefinition>ManufacturingSupplyChainSystem(x) ↔SupplyChainSystem(x) ∧ ∃p(MaterialProduct(p) ∧ hasSpecifiedOutput(x,p))</iof-av:firstOrderLogicDefinition>
        <iof-av:naturalLanguageDefinition>supply chain system that has one or more material products as the output</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>Manufacturing Supply Chain System is a Supply Chain System that &apos;has specified output&apos; some Material Product</iof-av:semiFormalNaturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/MaterialArtifactModelName -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/MaterialArtifactModelName">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/NameIdentifier"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/designates"/>
                        <owl:someValuesFrom rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/MaterialArtifact"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:label>material artifact model name</rdfs:label>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</iof-av:isPrimitive>
        <iof-av:logicAxiom>
            <rdf:Description>
                <iof-av:firstOrderLogicAxiom>MaterialArtifactModelName(x)  → NameIdentifiert (x) ∧ ∃y (MaterialArtifact (y) ∧ designates(x,y))</iof-av:firstOrderLogicAxiom>
                <iof-av:semiFormalNaturalLanguageAxiom>Material Artifact Model Name x implies x is a Name Identifier and &apos;designates&apos; some Material Artifact y</iof-av:semiFormalNaturalLanguageAxiom>
            </rdf:Description>
        </iof-av:logicAxiom>
        <iof-av:naturalLanguageDefinition>name identifier that identifies an artifact model name</iof-av:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/MotionProcess -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/MotionProcess">
        <rdfs:subClassOf rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/PlannedProcess"/>
        <rdfs:label>motion process</rdfs:label>
        <iof-av:adaptedFrom>http://www.ontologyrepository.com/CommonCoreOntologies/Mid/EventOntology</iof-av:adaptedFrom>
        <iof-av:naturalLanguageDefinition>planned process by which an agent causes the position or location of some material entity to change</iof-av:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/NAICSCode -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/NAICSCode">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/CodeIdentifier"/>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/designates"/>
                                <owl:someValuesFrom>
                                    <owl:Class>
                                        <owl:intersectionOf rdf:parseType="Collection">
                                            <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/BusinessFunction"/>
                                            <owl:Restriction>
                                                <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/functionOf"/>
                                                <owl:someValuesFrom rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/BusinessOrganization"/>
                                            </owl:Restriction>
                                        </owl:intersectionOf>
                                    </owl:Class>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/prescribedBy"/>
                                <owl:someValuesFrom rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/NAICSCodingSystem"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/CodeIdentifier"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/designates"/>
                <owl:allValuesFrom rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/BusinessOrganization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>north american indusry classification system code</rdfs:label>
        <skos:example>312111: Soft Drink Manufacturing, 326211: Tire Manufacturing, Bridgestone Americas Inc is designated by 2 NAICS Codes:  332613 (Spring Manufacturing) and 326211 (Tire Manufacturing)</skos:example>
        <iof-av:abbreviation>NAICS Code</iof-av:abbreviation>
        <iof-av:explanatoryNote>A company can have multiple NAICS codes</iof-av:explanatoryNote>
        <iof-av:explanatoryNote>The U.S. Census Bureau assigns one NAICS code to each establishment based on its primary activity (the activity that generates the most revenue for the establishment) to collect, tabulate, analyze, and disseminate statistical data describing the economy of the United States. A business organization might have multiple NAICS codes.</iof-av:explanatoryNote>
        <iof-av:firstOrderLogicDefinition>NAICSCoder(x) ↔CodeIdentifier (x) ∧ ∃y∃z (Function(y) ∧ BusinessOrganization (z) ∧ functionOf (y,z) ∧ designates(x,y)) ∧ (∃q (NAICSCodingSystem (q) ∧ prescribedBy (x,q)))</iof-av:firstOrderLogicDefinition>
        <iof-av:naturalLanguageDefinition>code Identifier that uniquely identifies the primary business function of an organization and that is assigned according to NAICS coding system</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>NAICS Code Identifier is a Code identifier that &apos;designates&apos; the Business Function of some Business Organization and that is &apos;prescribed by&apos; NAICS Coding System</iof-av:semiFormalNaturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/NAICSCodingSystem -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/NAICSCodingSystem">
        <rdfs:subClassOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/CodingSystem"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/CodingSystem"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/prescribes"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/CodeIdentifier"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/designates"/>
                                        <owl:someValuesFrom rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/BusinessFunction"/>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:label>naics coding system</rdfs:label>
        <skos:altLabel>north american industry classification system coding scheme</skos:altLabel>
        <iof-av:explanatoryNote>NAICS is a self-assigned coding system. What this means is a company selects the code that best depicts their primary business activity and then uses it when asked for their code</iof-av:explanatoryNote>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</iof-av:isPrimitive>
        <iof-av:logicAxiom>
            <rdf:Description>
                <iof-av:firstOrderLogicAxiom>NAICSCodingSystem(x) →InformationContent Entity(x) ∧ ∃y(CodeIdentifier (y) ∧ prescribes (x,y)) ∧ ∃b(BusinessFunction (b) ∧ designates (y,b))</iof-av:firstOrderLogicAxiom>
                <iof-av:semiFormalNaturalLanguageAxiom>NAICS Coding System x implies x is an Information Content Entity and x prescribes some Code Identifier y that designates a Business Function b</iof-av:semiFormalNaturalLanguageAxiom>
            </rdf:Description>
        </iof-av:logicAxiom>
        <iof-av:naturalLanguageDefinition>coding system that prescribes a NAICS code identifier</iof-av:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/NameIdentifier -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/NameIdentifier">
        <rdfs:subClassOf rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/InformationContentEntity"/>
        <rdfs:label>name identifier</rdfs:label>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</iof-av:isPrimitive>
        <iof-av:naturalLanguageDefinition>information entity that consists of a string of characters and identifies an entity within a specified cultural, social, or technical namespace</iof-av:naturalLanguageDefinition>
        <iof-av:primitiveRationale>The required constructs for creation of a formal definition are not available in this release.</iof-av:primitiveRationale>
    </owl:Class>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ObservingProcess -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ObservingProcess">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/PlannedProcess"/>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000057"/>
                                <owl:someValuesFrom>
                                    <owl:Class>
                                        <owl:intersectionOf rdf:parseType="Collection">
                                            <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000040"/>
                                            <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/Agent"/>
                                        </owl:intersectionOf>
                                    </owl:Class>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/hasOutput"/>
                                <owl:someValuesFrom rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/InformationContentEntity"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:label>observing process</rdfs:label>
        <skos:editorialNote>this is based on the tentative definition provided in MTConnect ontology</skos:editorialNote>
        <iof-av:naturalLanguageDefinition>planned process in which an agent perceives or detects a material entity and selects it as worthy of observation, and which has as output some information content entity about the selected material entity</iof-av:naturalLanguageDefinition>
        <iof-av:naturalLanguageDefinition>process in which an agent perceives some aspect of a material entity</iof-av:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/Organism -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/Organism">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000030"/>
        <rdfs:label>organism</rdfs:label>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</iof-av:isPrimitive>
        <iof-av:naturalLanguageDefinition>object made up of one or more cells and able to carry on the activities of life</iof-av:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/OrganizationCapability -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/OrganizationCapability">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/Capability"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/capabilityOf"/>
                        <owl:someValuesFrom rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/BusinessOrganization"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/Capability"/>
        <rdfs:label>organization capability</rdfs:label>
        <iof-av:adaptedFrom>https://www.sebokwiki.org/wiki/Organizational_Capability_(glossary)</iof-av:adaptedFrom>
        <iof-av:directSource>CCO</iof-av:directSource>
        <iof-av:firstOrderLogicDefinition>OrganizationCapability(x) ↔ Capability(x) ∧ ∃o(Organization(o)∧capabilityOf(x,o))</iof-av:firstOrderLogicDefinition>
        <iof-av:naturalLanguageDefinition>capability of a business organization to perform certain actions that contribute towards organizational objectives</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>Organization Capability is a Capability that is the &apos;capability of&apos; some Business Organization</iof-av:semiFormalNaturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/PackagingPlanSpecification -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/PackagingPlanSpecification">
        <rdfs:subClassOf rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/PlanSpecification"/>
        <rdfs:label>packaging plan specification</rdfs:label>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</iof-av:isPrimitive>
        <iof-av:logicAxiom>
            <rdf:Description>
                <iof-av:firstOrderLogicAxiom>PlanSpecification(x) ∧ ∃c(PackagingProcess(p) ∧ prescribes(x,p)) → PackagingPlanSpecification(x)</iof-av:firstOrderLogicAxiom>
                <iof-av:semiFormalNaturalLanguageAxiom>x is a Plan Specification that &apos;prescribes&apos; some Packaging Process p and &apos;has participant at some time&apos; some &apos;material product&apos; m implies x is a Packaging Plan Specification</iof-av:semiFormalNaturalLanguageAxiom>
            </rdf:Description>
        </iof-av:logicAxiom>
        <iof-av:naturalLanguageDefinition>plan specification that prescribes a packaging process</iof-av:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/PackagingProcess -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/PackagingProcess">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/BusinessProcess"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000057"/>
                        <owl:someValuesFrom rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/MaterialProduct"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/prescribedBy"/>
                        <owl:someValuesFrom rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/PackagingPlanSpecification"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:label>packaging process</rdfs:label>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</iof-av:isPrimitive>
        <iof-av:logicAxiom>
            <rdf:Description>
                <iof-av:firstOrderLogicDefinition>PackagingProcess(x)→BusinessProcess(x) ∧ ∃m, ∃p (MaterialProduct(m) ∧ PackagingPlanSpecification(p) ∧ hasParticipantAtSomeTime (x,m) ∧ prescribedBy(x,p))</iof-av:firstOrderLogicDefinition>
                <iof-av:semiFormalNaturalLanguageAxiom>Packaging Process x implies that x is a Business Process and &apos;has participant at some time&apos; some Material Procduct and x is &apos;prescribed by&apos; some Packaging Plan Specification</iof-av:semiFormalNaturalLanguageAxiom>
            </rdf:Description>
        </iof-av:logicAxiom>
        <iof-av:naturalLanguageDefinition>business process of providing a protective covering to a product (material products) in such a way that it protects the product during storage and movement</iof-av:naturalLanguageDefinition>
        <iof-av:subjectMatterExpertExplanation>Packing is the preparation of product or commodity for proper storage and/or transportation

packaging may entail blocking, bracing, cushioning, marking, sealing, labeling, strapping, weather proofing, wrapping</iof-av:subjectMatterExpertExplanation>
    </owl:Class>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/PackagingService -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/PackagingService">
        <rdfs:subClassOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/LogisticsService"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000117"/>
                <owl:someValuesFrom rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/PackagingProcess"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>packaging service</rdfs:label>
        <iof-av:logicAxiom>
            <rdf:Description>
                <iof-av:firstOrderLogicAxiom>PackagingService(x) → LogisticsService(x) ∧ ∃p (PackagingProcess(p) ∧ hasOccurentPart(x,p))</iof-av:firstOrderLogicAxiom>
                <iof-av:semiFormalNaturalLanguageAxiom>Packaging Service x implies x is a Logistics Service that &apos;has occurrent part&apos; some Packaging Process</iof-av:semiFormalNaturalLanguageAxiom>
            </rdf:Description>
        </iof-av:logicAxiom>
        <iof-av:naturalLanguageDefinition>logistics service that consists of at lest one packaging process</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>Packaging Service x implies x is a Logistics Service that &apos;has occurrent part&apos; some Packaging Process</iof-av:semiFormalNaturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/PackagingServiceProviderRole -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/PackagingServiceProviderRole">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/LogisticsServiceProvider"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/roleOf"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <owl:Class>
                                        <owl:unionOf rdf:parseType="Collection">
                                            <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/BusinessOrganization"/>
                                            <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/Person"/>
                                        </owl:unionOf>
                                    </owl:Class>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/agentIn"/>
                                        <owl:someValuesFrom rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/PackagingService"/>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:label>packaging service provider role</rdfs:label>
        <iof-av:firstOrderLogicDefinition>PackagingServiceProviderRole(x) ↔  LogisticsServiceProviderRole(x) ∧ ∃y,∃p ((BusinessOrganization(y) ∨ Person(y)) ∧ PackagingService(p) ∧ agentIn(y,p) ∧ roleOf(x, y))</iof-av:firstOrderLogicDefinition>
        <iof-av:naturalLanguageDefinition>logisitics service provider role held by a business organization or person when it provides a packaging service</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>Packaging Service Provider Role x implies x is a Logistics Service Provider Role that is the &apos;role of&apos; a Business Organization or Person y when it is&apos;agent in&apos; some Packaging Service</iof-av:semiFormalNaturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/PartNumber -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/PartNumber">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/CodeIdentifier"/>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/designates"/>
                                <owl:someValuesFrom rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/MaterialArtifact"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/prescribedBy"/>
                                <owl:someValuesFrom rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/CodingSystem"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/CodeIdentifier"/>
        <rdfs:label>part number</rdfs:label>
        <iof-av:firstOrderLogicDefinition>PartNumber(x) ↔CodeIdentifier (x) ∧ ∃y (MaterialArtifact(y) ∧ designates(x,y)) ∧ ∃z (CodingSystem (z) ∧ prescribedBy (x,z))</iof-av:firstOrderLogicDefinition>
        <iof-av:naturalLanguageDefinition>code identifier that uniquely identifies an artifact (material artifact) and that is created according to some part numbering system</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>&apos;part identifier&apos; is a  &apos;code identifier&apos; that &apos;designates&apos; some &apos;material artifact&apos; and is &apos;prescribed by&apos; some &apos;coding system&apos;</iof-av:semiFormalNaturalLanguageDefinition>
        <iof-av:synonym>item number</iof-av:synonym>
        <iof-av:usageNote>a part/item number can be applied to raw materials, fluids, component parts, subassemblies, finished assemblies, packaging, etc.</iof-av:usageNote>
    </owl:Class>
    


    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/PieceofTransportEquipment -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/PieceofTransportEquipment">
        <rdfs:subClassOf rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/MaterialArtifact"/>
