    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#ActofReverseEngineering -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#ActofReverseEngineering">
        <rdfs:subClassOf rdf:resource="http://www.industrialontologies.org/core#PlannedProcess"/>
        <rdfs:label>Act of Reverse Engineering</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#AgentCapability -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#AgentCapability">
        <rdfs:subClassOf rdf:resource="http://www.industrialontologies.org/core#Capability"/>
        <IOFAnnotationVocabulary:directSource>CCO</IOFAnnotationVocabulary:directSource>
        <IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>A Capability that inheres in an Agent to the extent of that Agent&apos;s capacity to realize it in Actions of a certain type.</IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>
        <rdfs:label>&apos;Agent Capability&apos;</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#Airway -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#Airway">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000028"/>
        <IOFAnnotationVocabulary:naturalLanguageDefinition>A spatia region that connects one specified location to another at a specified altitude, along which an aircraft that meets the requirements of the airway may be flown.</IOFAnnotationVocabulary:naturalLanguageDefinition>
        <rdfs:label>Airway</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#Cargo -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#Cargo">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000040"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/bearer_of"/>
                        <owl:someValuesFrom rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology#CargoRole"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000040"/>
        <IOFAnnotationVocabulary:naturalLanguageDefinition>Goods carried on a ship, aircraft, or motor vehicle.</IOFAnnotationVocabulary:naturalLanguageDefinition>
        <IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>A material entity that bears a cargo role.</IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>
        <rdfs:label>Cargo</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#CargoRole -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#CargoRole">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000023"/>
        <IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>A Role borne by a material entity when transported by a Transport Equipment.</IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>
        <rdfs:label>Cargo Role</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#CastingService -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#CastingService">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology#ManufacturingService"/>
        <CommonCoreOntologies:definition>A Manufacturing Service which includes at least one casting process as process part.</CommonCoreOntologies:definition>
        <rdfs:label>Casting Service</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#CommercialService -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#CommercialService">
        <rdfs:subClassOf rdf:resource="http://www.industrialontologies.org/core#PlannedProcess"/>
        <IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>Planned Process that has proper-occurrent-parts, some Act of Service Provisioning and some an Act of Consumption that coincide temporally, and that is prescribed by a Service Agreement, and in which some Agents playing a causal role in the respective occurrent parts, are parties of the agreement.</IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>
        <rdfs:label>Commercial Service</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#EngineeringDesignCapability -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#EngineeringDesignCapability">
        <rdfs:subClassOf rdf:resource="http://www.industrialontologies.org/core#Capability"/>
        <IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>A Capability that inheres in an Engineered System, Organization, or Person to design Material Artifacts as prescribed by engineering rules and principles.</IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>
        <rdfs:label>Engineering Design Capability</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#GeometricModelingArtifactFunction -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#GeometricModelingArtifactFunction">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000034"/>
        <rdfs:label>Geometric Modeling Artifact Function</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#GeometricModelingSkill -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#GeometricModelingSkill">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology#Skill"/>
        <IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>A Skill that inheres in a Person and is realized in an Act of Geometric Modeling.</IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>
        <rdfs:label>Geometric Modeling Skill</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#LowVolumeProductionCapability -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#LowVolumeProductionCapability">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/ontology/ProductionCapability"/>
        <IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>A Production Capability to manufacture large quantities of some material artifact in some unit of time.</IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>
        <rdfs:label>Low Volume Production Capability</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#MachiningService -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#MachiningService">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology#ManufacturingService"/>
        <CommonCoreOntologies:definition>A Manufacturing Service includes at least one machining process as proess part.</CommonCoreOntologies:definition>
        <rdfs:label>Machining Service</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#ManufacturingService -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#ManufacturingService">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology#CommercialService"/>
        <IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>A Service which 1) includes at least one manufacturing process as process part and 2) an Agent satisfies the manufacturing needs of another agent.</IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>
        <rdfs:label>Manufacturing Service</rdfs:label>
        <skos:example>Foxconn, a contract manufacturing service company, owns and operates factories in China that do the final assembly of iPhones for Apple.</skos:example>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#ManufacturingSupplyChain -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#ManufacturingSupplyChain">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology/SupplyChainSystem"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/has_output_at_some_time"/>
                        <owl:someValuesFrom rdf:resource="http://www.industrialontologies.org/core#Product"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>A Supply Chain System whose members are agent in some Product Production Process that outputs some Product.</IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>
        <rdfs:label>Manufacturing Supply Chain System</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#OrganizationCapability -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#OrganizationCapability">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology#AgentCapability"/>
        <IOFAnnotationVocabulary:adaptedFrom>https://www.sebokwiki.org/wiki/Organizational_Capability_(glossary)</IOFAnnotationVocabulary:adaptedFrom>
        <IOFAnnotationVocabulary:directSource>CCO</IOFAnnotationVocabulary:directSource>
        <IOFAnnotationVocabulary:naturalLanguageDefinition>The capability of an organization to perform certain acts that contributes towards organizational value and purpose.</IOFAnnotationVocabulary:naturalLanguageDefinition>
        <IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>An Agent Capability that inheres in an Organization to the extent of that Organization&apos;s capacity to realize it in Actions of a certain type.</IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition>
        <rdfs:label>Organization Capability</rdfs:label>
    </owl:Class>
    


