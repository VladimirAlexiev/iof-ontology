@prefix : <http://infoneer.txstate.edu/IOF/SupplyChainOntology#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix iof: <http://www.industrialontologies.org/core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix scro: <http://infoneer.txstate.edu/IOF/SupplyChainOntology#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix oboInOwl: <http://www.geneontology.org/formats/oboInOwl#> .
@prefix CommonCoreOntologies: <http://www.ontologyrepository.com/CommonCoreOntologies/> .
@prefix IOFAnnotationVocabulary: <http://www.industrialontologies.org/IOFAnnotationVocabulary/> .
@base <http://infoneer.txstate.edu/IOF/SupplyChainOntology> .

<http://infoneer.txstate.edu/IOF/SupplyChainOntology> rdf:type owl:Ontology ;
                                                       owl:imports <http://www.industrialontologies.org/core/> ;
                                                       bfo:IAO_0000232 "This is a draft ontology. Not formally released yet." ;
                                                       dc:contributor "Farhad Ameri" ;
                                                       terms:license "https://opensource.org/licenses/MIT" ;
                                                       terms:title "Supply Chain Reference Ontology" ;
                                                       rdfs:comment "This ontology is designed in the context of IOF Supply Chain WG activities" ;
                                                       owl:versionInfo 1.0 .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.obolibrary.org/obo/IAO_0000115
bfo:IAO_0000115 rdf:type owl:AnnotationProperty ;
                rdfs:label "definition" .


###  http://purl.obolibrary.org/obo/IAO_0000232
bfo:IAO_0000232 rdf:type owl:AnnotationProperty ;
                rdfs:label "curator note" .


###  http://purl.org/dc/elements/1.1/contributor
dc:contributor rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/source
dc:source rdf:type owl:AnnotationProperty .


###  http://www.ontologyrepository.com/CommonCoreOntologies/alternative_label
CommonCoreOntologies:alternative_label rdf:type owl:AnnotationProperty .


###  http://www.ontologyrepository.com/CommonCoreOntologies/definition
CommonCoreOntologies:definition rdf:type owl:AnnotationProperty .


###  http://www.ontologyrepository.com/CommonCoreOntologies/definition_source
CommonCoreOntologies:definition_source rdf:type owl:AnnotationProperty .


###  http://www.ontologyrepository.com/CommonCoreOntologies/elucidation
CommonCoreOntologies:elucidation rdf:type owl:AnnotationProperty .


###  http://www.ontologyrepository.com/CommonCoreOntologies/example_of_usage
CommonCoreOntologies:example_of_usage rdf:type owl:AnnotationProperty .


###  http://www.ontologyrepository.com/CommonCoreOntologies/is_curated_in_ontology
CommonCoreOntologies:is_curated_in_ontology rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#OccurrentPartOf
scro:OccurrentPartOf rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf bfo:BFO_0000050 ;
                     owl:inverseOf scro:has_occurrent_part ;
                     rdfs:label "occurrent part of"^^xsd:string .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#has_capability
scro:has_capability rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf iof:RO_0000091 ;
                    rdfs:domain bfo:BFO_0000040 ;
                    rdfs:label "has capability" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#has_occurrent_part
scro:has_occurrent_part rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf bfo:BFO_0000051 ;
                        rdfs:domain bfo:BFO_0000003 ;
                        rdfs:range bfo:BFO_0000003 .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#has_skill
scro:has_skill rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf scro:has_capability ;
               rdfs:range <http://infoneer.txstate.edu/IOF/SupplyChainOntology/Skill> ;
               rdfs:label "has skill" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#is_prescribed_by
scro:is_prescribed_by rdf:type owl:ObjectProperty ;
                      owl:inverseOf CommonCoreOntologies:prescribes .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology/agent_in
<http://infoneer.txstate.edu/IOF/SupplyChainOntology/agent_in> rdf:type owl:ObjectProperty ;
                                                               rdfs:subPropertyOf bfo:RO_0000056 .


###  http://purl.obolibrary.org/obo/BFO_0000050
bfo:BFO_0000050 rdf:type owl:ObjectProperty ;
                rdfs:comment "part of" ;
                rdfs:label "part of" .


###  http://purl.obolibrary.org/obo/BFO_0000051
bfo:BFO_0000051 rdf:type owl:ObjectProperty ;
                rdfs:comment "has part" ;
                rdfs:label "has part" .


###  http://purl.obolibrary.org/obo/RO_0000056
bfo:RO_0000056 rdf:type owl:ObjectProperty ;
               bfo:IAO_0000115 "a relation between a continuant and a process, in which the continuant is somehow involved in the process" ;
               rdfs:label "participates in" .


###  http://www.industrialontologies.org/core#RO_0000091
iof:RO_0000091 rdf:type owl:ObjectProperty .


###  http://www.ontologyrepository.com/CommonCoreOntologies/bearer_of
CommonCoreOntologies:bearer_of rdf:type owl:ObjectProperty .


###  http://www.ontologyrepository.com/CommonCoreOntologies/designates
CommonCoreOntologies:designates rdf:type owl:ObjectProperty .


###  http://www.ontologyrepository.com/CommonCoreOntologies/has_output_at_some_time
CommonCoreOntologies:has_output_at_some_time rdf:type owl:ObjectProperty .


###  http://www.ontologyrepository.com/CommonCoreOntologies/is_about
CommonCoreOntologies:is_about rdf:type owl:ObjectProperty .


###  http://www.ontologyrepository.com/CommonCoreOntologies/is_delimited_by
CommonCoreOntologies:is_delimited_by rdf:type owl:ObjectProperty .


###  http://www.ontologyrepository.com/CommonCoreOntologies/prescribes
CommonCoreOntologies:prescribes rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#ActofReverseEngineering
scro:ActofReverseEngineering rdf:type owl:Class ;
                             rdfs:subClassOf <http://www.industrialontologies.org/core/PlannedProcess> ;
                             rdfs:label "Act of Reverse Engineering" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#AgentCapability
scro:AgentCapability rdf:type owl:Class ;
                     rdfs:subClassOf <http://www.industrialontologies.org/core/Capability> ;
                     IOFAnnotationVocabulary:directSource "CCO" ;
                     IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Capability that inheres in an Agent to the extent of that Agent's capacity to realize it in Actions of a certain type." ;
                     rdfs:label "'Agent Capability'" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#Airway
scro:Airway rdf:type owl:Class ;
            rdfs:subClassOf bfo:BFO_0000028 ;
            IOFAnnotationVocabulary:naturalLanguageDefinition "A spatia region that connects one specified location to another at a specified altitude, along which an aircraft that meets the requirements of the airway may be flown." ;
            rdfs:label "Airway" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#Cargo
scro:Cargo rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( bfo:BFO_0000040
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty CommonCoreOntologies:bearer_of ;
                                                        owl:someValuesFrom scro:CargoRole
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf bfo:BFO_0000040 ;
           IOFAnnotationVocabulary:naturalLanguageDefinition "Goods carried on a ship, aircraft, or motor vehicle." ;
           IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A material entity that bears a cargo role." ;
           rdfs:label "Cargo" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#CargoRole
scro:CargoRole rdf:type owl:Class ;
               rdfs:subClassOf bfo:BFO_0000023 ;
               IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Role borne by a material entity when transported by a Transport Equipment." ;
               rdfs:label "Cargo Role" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#CastingService
scro:CastingService rdf:type owl:Class ;
                    rdfs:subClassOf scro:ManufacturingService ;
                    CommonCoreOntologies:definition "A Manufacturing Service which includes at least one casting process as process part." ;
                    rdfs:label "Casting Service" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#CommercialService
scro:CommercialService rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.industrialontologies.org/core/PlannedProcess> ;
                       IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "Planned Process that has proper-occurrent-parts, some Act of Service Provisioning and some an Act of Consumption that coincide temporally, and that is prescribed by a Service Agreement, and in which some Agents playing causal roles in the respective occurrent parts, are parties of the agreement." ;
                       rdfs:label "Commercial Service" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#Department
scro:Department rdf:type owl:Class ;
                rdfs:subClassOf scro:GroupOfPersons ;
                IOFAnnotationVocabulary:naturalLanguageDefinition "A Group of Persons that is part of some organization which is created to provide certain functions and achieve some objective of the organization and appears in the organizational hierarchy of that organization." ;
                rdfs:label "Department" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#EngineeringDesignCapability
scro:EngineeringDesignCapability rdf:type owl:Class ;
                                 rdfs:subClassOf <http://www.industrialontologies.org/core/Capability> ;
                                 IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Capability that inheres in an Engineered System, Organization, or Person to design Material Artifacts as prescribed by engineering rules and principles." ;
                                 rdfs:label "Engineering Design Capability" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#GeometricModelingArtifactFunction
scro:GeometricModelingArtifactFunction rdf:type owl:Class ;
                                       rdfs:subClassOf CommonCoreOntologies:ArtifactFunction ;
                                       rdfs:label "Geometric Modeling Artifact Function" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#GeometricModelingSkill
scro:GeometricModelingSkill rdf:type owl:Class ;
                            rdfs:subClassOf <http://infoneer.txstate.edu/IOF/SupplyChainOntology/Skill> ;
                            IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Skill that inheres in a Person and is realized in an Act of Geometric Modeling." ;
                            rdfs:label "Geometric Modeling Skill" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#GroupOfPersons
scro:GroupOfPersons rdf:type owl:Class ;
                    rdfs:subClassOf CommonCoreOntologies:GroupOfAgents ;
                    IOFAnnotationVocabulary:directSource "CCO" ;
                    IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Group of Agents that has only Persons as parts." ;
                    rdfs:label "Group of Persons" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#LowVolumeProductionCapability
scro:LowVolumeProductionCapability rdf:type owl:Class ;
                                   rdfs:subClassOf <http://infoneer.txstate.edu/ontology/ProductionCapability> ;
                                   IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Production Capability to manufacture large quantities of some material artifact in some unit of time." ;
                                   rdfs:label "Low Volume Production Capability" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#MachiningService
scro:MachiningService rdf:type owl:Class ;
                      rdfs:subClassOf scro:ManufacturingService ;
                      CommonCoreOntologies:definition "A Manufacturing Service includes at least one machining process as proess part." ;
                      rdfs:label "Machining Service" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#ManufacturingService
scro:ManufacturingService rdf:type owl:Class ;
                          rdfs:subClassOf scro:CommercialService ;
                          IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Commercial Service which 1) includes at least one manufacturing process as process part and 2) an Agent satisfies the manufacturing needs of another agent." ;
                          rdfs:label "Manufacturing Service" ;
                          skos:example "Foxconn, a contract manufacturing service company, owns and operates factories in China that do the final assembly of iPhones for Apple." .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#ManufacturingSupplyChain
scro:ManufacturingSupplyChain rdf:type owl:Class ;
                              rdfs:subClassOf <http://infoneer.txstate.edu/IOF/SupplyChainOntology/SupplyChainSystem> ;
                              IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Supply Chain System whose members are agent in some Product Production Process that outputs some Manufactured Product." ;
                              rdfs:label "Manufacturing Supply Chain System" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#ReverseEngineeringCapability
scro:ReverseEngineeringCapability rdf:type owl:Class ;
                                  rdfs:subClassOf scro:EngineeringDesignCapability ;
                                  IOFAnnotationVocabulary:naturalLanguageDefinition "The process by which a man-made object is deconstructed to reveal its designs, architecture, code or to extract knowledge from the object." ;
                                  IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "An Engineering Design Capability to examine and analyze a Manufactured Product in ordder to identify its Engineering Specifications." ;
                                  rdfs:label "Reverse Engineering Capability" ;
                                  skos:altLabel "Back Engineering" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#ScanningArtifactFunction
scro:ScanningArtifactFunction rdf:type owl:Class ;
                              rdfs:subClassOf CommonCoreOntologies:ArtifactFunction ;
                              rdfs:label "Scanning Artifact Function" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#Seaway
scro:Seaway rdf:type owl:Class ;
            rdfs:subClassOf bfo:BFO_0000028 ;
            IOFAnnotationVocabulary:naturalLanguageDefinition "A spatia region that connects one specified location to another on a body of water along which a watercraft may travel." ;
            rdfs:label "Seaway" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#ServiceConsumerRole
scro:ServiceConsumerRole rdf:type owl:Class ;
                         rdfs:subClassOf <http://infoneer.txstate.edu/IOF/SupplyChainOntology/CustomerRole> ;
                         IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A role which inheres in a person or organization who uses a service" ;
                         rdfs:label "Service Consumer Role" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#ServiceSpecification
scro:ServiceSpecification rdf:type owl:Class ;
                          rdfs:subClassOf CommonCoreOntologies:DirectiveInformationContentEntity ;
                          IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Directive Information Content entity with service action specifications and service objective specifications as parts that, when concretized, is realized in a process comprised of production and consumption of an economic good such that production and consumption coincide." ;
                          rdfs:label "Service Plan Specification" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#ServiceSupplyChain
scro:ServiceSupplyChain rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( <http://infoneer.txstate.edu/IOF/SupplyChainOntology/SupplyChainSystem>
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty CommonCoreOntologies:has_output_at_some_time ;
                                                                     owl:someValuesFrom scro:CommercialService
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] ;
                        rdfs:subClassOf <http://infoneer.txstate.edu/IOF/SupplyChainOntology/SupplyChainSystem> ;
                        IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Supply Chain System whose members are agent in some Service Provisioning Process that outputs some Commercial Service." ;
                        rdfs:label "Service Supply Chain System" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#Team
scro:Team rdf:type owl:Class ;
          rdfs:subClassOf scro:GroupOfPersons ;
          IOFAnnotationVocabulary:naturalLanguageDefinition "A Group of Persons that is be part of some department or organization which is formed to provide  certain functions to achieve some objective of the department or organization." ;
          rdfs:label "Team" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#TransportationService
scro:TransportationService rdf:type owl:Class ;
                           rdfs:subClassOf scro:CommercialService ;
                           IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Commercial Service in which an Agent changes the location of some material entities to satisfy the transportation needs of another Agent." ;
                           rdfs:label "Transportation Service" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology#3DPrintingArtifactFunction
<http://infoneer.txstate.edu/IOF/SupplyChainOntology#3DPrintingArtifactFunction> rdf:type owl:Class ;
                                                                                 rdfs:subClassOf CommonCoreOntologies:ArtifactFunction ;
                                                                                 rdfs:label "3D Printing Artifact Function" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology/ActOfTransportation
<http://infoneer.txstate.edu/IOF/SupplyChainOntology/ActOfTransportation> rdf:type owl:Class ;
                                                                          rdfs:subClassOf CommonCoreOntologies:ActOfLocationChange ;
                                                                          IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "An Act of Location Change involving the movement of material entities through some Transportation Agent." ;
                                                                          CommonCoreOntologies:alternative_label "act of shipping" ;
                                                                          rdfs:label "Act of Transportation" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology/ActofPositionChange
<http://infoneer.txstate.edu/IOF/SupplyChainOntology/ActofPositionChange> rdf:type owl:Class ;
                                                                          rdfs:subClassOf CommonCoreOntologies:ActOfMotion ;
                                                                          IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "An Act of Motion in which the position (i.e. the orientation, alignment, or arrangement) of some Object or of one or more of an Object's parts is changed by some Agent." ;
                                                                          rdfs:comment "An Act of Position Change does not entail a change of location." ;
                                                                          rdfs:label "Act of Position Change" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology/ActofSupplierEvaluation
<http://infoneer.txstate.edu/IOF/SupplyChainOntology/ActofSupplierEvaluation> rdf:type owl:Class ;
                                                                              rdfs:subClassOf CommonCoreOntologies:ActOfAppraisal ;
                                                                              IOFAnnotationVocabulary:naturalLanguageDefinition "An Act of Measuring that involves evaluating, assessing, estimating, or judging the capabilities of an Agent that is intended to become a member of a Supply Chain." ;
                                                                              rdfs:label "Act of Supplier Evaluation" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology/AircraftManufacturingFactory
<http://infoneer.txstate.edu/IOF/SupplyChainOntology/AircraftManufacturingFactory> rdf:type owl:Class ;
                                                                                   rdfs:subClassOf <http://infoneer.txstate.edu/IOF/SupplyChainOntology/Factory> ;
                                                                                   IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Facility that is designed for manufacturing aircraft." ;
                                                                                   rdfs:label "Aircraft Manufacturing Factory" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology/AssemblyServiceProviderRole
<http://infoneer.txstate.edu/IOF/SupplyChainOntology/AssemblyServiceProviderRole> rdf:type owl:Class ;
                                                                                  rdfs:subClassOf <http://infoneer.txstate.edu/IOF/SupplyChainOntology/ManufacturingServiceProviderRole> ;
                                                                                  IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Service Provider Role that is borne by an Agent to provide an Assembly Service to some Agents in some Supply Chain." ;
                                                                                  rdfs:label "Assembly Service Provider Role" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology/Batch
<http://infoneer.txstate.edu/IOF/SupplyChainOntology/Batch> rdf:type owl:Class ;
                                                            rdfs:subClassOf <http://infoneer.txstate.edu/IOF/SupplyChainOntology/TraceableResourceUnit> ;
                                                            IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Traceable Resource Unit that is the output of a batch process" ;
                                                            rdfs:label "Batch" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology/BusinessFunction
<http://infoneer.txstate.edu/IOF/SupplyChainOntology/BusinessFunction> rdf:type owl:Class ;
                                                                       rdfs:subClassOf bfo:BFO_0000034 ;
                                                                       IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Function that inheres in some Business Organization in virtue of that Business Organization being designed to be engaged in Planned Processes that require that Function to be realized and 2) the function exists because it is prescribed by some objectives that 'is carried by' the organization." ;
                                                                       rdfs:comment "agent that bears the function should be linked to the agent that prescribes the objective." ;
                                                                       rdfs:label "Business Function" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology/CastingServiceProviderRole
<http://infoneer.txstate.edu/IOF/SupplyChainOntology/CastingServiceProviderRole> rdf:type owl:Class ;
                                                                                 rdfs:subClassOf <http://infoneer.txstate.edu/IOF/SupplyChainOntology/ManufacturingServiceProviderRole> ;
                                                                                 IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Manufacturing Service Provider Role that is borne by an Agent to provide a Casting Service to some Agents in some Supply Chain." ;
                                                                                 rdfs:label "Casting Service Provider Role" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology/CustomerRole
<http://infoneer.txstate.edu/IOF/SupplyChainOntology/CustomerRole> rdf:type owl:Class ;
                                                                   rdfs:subClassOf bfo:BFO_0000023 ;
                                                                   IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Role that is borne by an Agent to buy or consume a Product or Service." ;
                                                                   rdfs:label "Customer Role" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology/DistributorAgent
<http://infoneer.txstate.edu/IOF/SupplyChainOntology/DistributorAgent> rdf:type owl:Class ;
                                                                       owl:equivalentClass [ owl:intersectionOf ( <http://infoneer.txstate.edu/IOF/SupplyChainOntology/SupplyChainAgent>
                                                                                                                  [ rdf:type owl:Restriction ;
                                                                                                                    owl:onProperty CommonCoreOntologies:bearer_of ;
                                                                                                                    owl:someValuesFrom <http://infoneer.txstate.edu/IOF/SupplyChainOntology/DistributorRole>
                                                                                                                  ]
                                                                                                                ) ;
                                                                                             rdf:type owl:Class
                                                                                           ] ;
                                                                       rdfs:subClassOf <http://infoneer.txstate.edu/IOF/SupplyChainOntology/SupplyChainAgent> ;
                                                                       IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Supplier that bears a Distributor Role in some supply chain." ;
                                                                       rdfs:label "Distributor Agent" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology/DistributorRole
<http://infoneer.txstate.edu/IOF/SupplyChainOntology/DistributorRole> rdf:type owl:Class ;
                                                                      rdfs:subClassOf <http://infoneer.txstate.edu/IOF/SupplyChainOntology/ProductSupplierRole> ;
                                                                      IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Product Supplier Role that is borne by an Agent when it buys  some Manufactured Product form a Manufacturer Agent and resells them to some Wholesaler Agents in a supply chain." ;
                                                                      rdfs:label "Distributor Role" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology/Facility
<http://infoneer.txstate.edu/IOF/SupplyChainOntology/Facility> rdf:type owl:Class ;
                                                               rdfs:subClassOf <http://www.industrialontologies.org/core/MaterialArtifact> ;
                                                               IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "An Artifact that is designed as a building or campus dedicated to some specific purpose." ;
                                                               rdfs:label "Facility" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology/Factory
<http://infoneer.txstate.edu/IOF/SupplyChainOntology/Factory> rdf:type owl:Class ;
                                                              rdfs:subClassOf <http://infoneer.txstate.edu/IOF/SupplyChainOntology/Facility> ;
                                                              IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Facility that is designed for manufacturing or refining discrete or continuous material products." ;
                                                              rdfs:label "Factory" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology/ForgingServiceProviderRole
<http://infoneer.txstate.edu/IOF/SupplyChainOntology/ForgingServiceProviderRole> rdf:type owl:Class ;
                                                                                 rdfs:subClassOf <http://infoneer.txstate.edu/IOF/SupplyChainOntology/ManufacturingServiceProviderRole> ;
                                                                                 IOFAnnotationVocabulary:semi-formalNaturalLanguageDefinition "A Manufacturing Service Provider Role that is borne by an Agent to provide a Forging Service to some Agents in some Supply Chain." ;
                                                                                 rdfs:label "Forging Service Provider Role" .


###  http://infoneer.txstate.edu/IOF/SupplyChainOntology/GroupOfOrganizations
<http://infoneer.txstate.edu/IOF/SupplyChainOntology/GroupOfOrganizations> rdf:type owl:Class ;
