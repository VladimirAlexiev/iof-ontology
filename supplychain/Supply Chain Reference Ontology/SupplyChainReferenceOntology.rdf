                                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000056"/>
                                <owl:someValuesFrom rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/InventoryManagementProcess"/>
