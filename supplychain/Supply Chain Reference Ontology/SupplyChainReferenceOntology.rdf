        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/roleOf"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/BusinessOrganization"/>
                                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/Person"/>
                                </owl:unionOf>
                            </owl:Class>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000056"/>
                                <owl:someValuesFrom rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/MachiningService"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>machining service provider role</rdfs:label>
        <iof-av:naturalLanguageDefinition>manufacturing service provider role that is held by a business organization or person when it provides a machining service</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>Machining Service Provider Role x implies that x is a Manufacturing Service Provider Role that is the &apos;role of&apos; some Business Organization or Person y when it &apos;participates in at some time&apos; some Machining Service m</iof-av:semiFormalNaturalLanguageDefinition>
    </owl:Class>



    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/Make-To-OrderProductionCapability -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/Make-To-OrderProductionCapability">
        <rdfs:subClassOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ProductionCapability"/>
        <rdfs:label>make-to-order production capability</rdfs:label>
        <iof-av:naturalLanguageDefinition>production capability to manufacture some material artifact only after a confirmed customer order is received</iof-av:naturalLanguageDefinition>
    </owl:Class>



    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/Make-To-StockProductionCapability -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/Make-To-StockProductionCapability">
        <rdfs:subClassOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ProductionCapability"/>
        <rdfs:label>make-to-stock production capability</rdfs:label>
        <iof-av:naturalLanguageDefinition>production capability to manufacture and store some material artifact to fulfill expected incoming orders or forcasted demand</iof-av:naturalLanguageDefinition>
    </owl:Class>



    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/Manufacturer -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/Manufacturer">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/BusinessOrganization"/>
                            <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/Person"/>
                        </owl:unionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/hasRole"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/ManufacturerRole"/>
                                    <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ManufacturingServiceProviderRole"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/Agent"/>
        <rdfs:label>manufacturer</rdfs:label>
        <iof-av:naturalLanguageDefinition>person or business organization who manufacturs products for sale or provides manufacturing services to other persons or organizations</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>Manufacturer is a Person or Business Organization that &apos;has role&apos; some Manufacturing Service Provider Role or Manufacturer Role</iof-av:semiFormalNaturalLanguageDefinition>
        <iof-av:subjectMatterExpertExplanation>Party that produces consumer packaged goods and sells them to retailers [GS1].</iof-av:subjectMatterExpertExplanation>
    </owl:Class>



    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ManufacturingService -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ManufacturingService">
        <rdfs:subClassOf rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/CommercialService"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000117"/>
                <owl:someValuesFrom rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/ManufacturingProcess"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>manufacturing service</rdfs:label>
        <skos:example>The final assembly service provided by Foxconn to Apple (Foxconn is a contract manufacturing service company that owns and operates factories in China that do the final assembly of iPhones for Apple), The machining services provided by a contract machinist</skos:example>
        <iof-av:naturalLanguageDefinition>commercial service that consists of at least one manufacturing process</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>Manufactuirng Service x implies x is a Commercial Service that &apos;has occurrent part&apos; some Manufacturing Process p</iof-av:semiFormalNaturalLanguageDefinition>
    </owl:Class>



    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ManufacturingServiceProviderRole -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ManufacturingServiceProviderRole">
        <rdfs:subClassOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ServiceProviderRole"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/roleOf"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/BusinessOrganization"/>
                                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/Person"/>
                                </owl:unionOf>
                            </owl:Class>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/agentIn"/>
                                <owl:someValuesFrom rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ManufacturingService"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>manufacturing service provider role</rdfs:label>
        <skos:example>a contract machinist who machines parts for another company</skos:example>
        <iof-av:naturalLanguageDefinition>service provider role that is held by a person or business organization when it provides a manufacturing service</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>Manufacturing Service Provider Role x implies that x is a Service Provider Role that is &apos;role of&apos; some Business Organization or Person y when it is &apos;agent in&apos; some Manufacturing Service m</iof-av:semiFormalNaturalLanguageDefinition>
    </owl:Class>



    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ManufacturingSupplyChain -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ManufacturingSupplyChain">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/SupplyChainSystem"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/hasSpecifiedOutput"/>
                        <owl:someValuesFrom rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/MaterialProduct"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/SupplyChainSystem"/>
        <rdfs:label>manufacturing supply chain system</rdfs:label>
        <iof-av:naturalLanguageDefinition>supply chain system that has one or more material products as the output</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>Manufacturing Supply Chain System is a Supply Chain System that &apos;has specified output&apos; some Material Product</iof-av:semiFormalNaturalLanguageDefinition>
    </owl:Class>



    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/MotionProcess -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/MotionProcess">
        <rdfs:subClassOf rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/PlannedProcess"/>
        <rdfs:label>motion process</rdfs:label>
        <iof-av:adaptedFrom>http://www.ontologyrepository.com/CommonCoreOntologies/Mid/EventOntology</iof-av:adaptedFrom>
        <iof-av:naturalLanguageDefinition>planned process by which an agent causes the position or location of some material entity to change</iof-av:naturalLanguageDefinition>
    </owl:Class>



    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/NAICSCode -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/NAICSCode">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/CodeIdentifier"/>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/designates"/>
                                <owl:someValuesFrom>
                                    <owl:Class>
                                        <owl:intersectionOf rdf:parseType="Collection">
                                            <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/BusinessFunction"/>
                                            <owl:Restriction>
                                                <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/functionOf"/>
                                                <owl:someValuesFrom rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/BusinessOrganization"/>
                                            </owl:Restriction>
                                        </owl:intersectionOf>
                                    </owl:Class>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/prescribedBy"/>
                                <owl:someValuesFrom rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/NAICSCodingSystem"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/CodeIdentifier"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/designates"/>
                <owl:allValuesFrom rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/BusinessOrganization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>NAICS code</rdfs:label>
        <skos:altLabel>North American Indusry Classification System code identifier</skos:altLabel>
        <skos:example>312111: Soft Drink Manufacturing, 326211: Tire Manufacturing, Bridgestone Americas Inc is designated by 2 NAICS Codes:  332613 (Spring Manufacturing) and 326211 (Tire Manufacturing)</skos:example>
        <iof-av:explanatoryNote>A company can have multiple NAICS codes</iof-av:explanatoryNote>
        <iof-av:explanatoryNote>The U.S. Census Bureau assigns one NAICS code to each establishment based on its primary activity (the activity that generates the most revenue for the establishment) to collect, tabulate, analyze, and disseminate statistical data describing the economy of the United States. A business organization might have multiple NAICS codes.</iof-av:explanatoryNote>
        <iof-av:naturalLanguageDefinition>code Identifier that uniquely identifies the primary business function of an organization and that is assigned according to NAICS coding system</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>NAICS Code Identifier is a Code identifier that &apos;designates&apos; the Business Function of some Business Organization and that is &apos;prescribed by&apos; NAICS Coding System</iof-av:semiFormalNaturalLanguageDefinition>
    </owl:Class>



    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/NAICSCodingSystem -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/NAICSCodingSystem">
        <rdfs:subClassOf rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/CodingSystem"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/CodingSystem"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/prescribes"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/CodeIdentifier"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/designates"/>
                                        <owl:someValuesFrom rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/BusinessFunction"/>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:label>NAICS coding system</rdfs:label>
        <iof-av:explanatoryNote>NAICS is a self-assigned coding system. What this means is a company selects the code that best depicts their primary business activity and then uses it when asked for their code</iof-av:explanatoryNote>
        <iof-av:naturalLanguageDefinition>coding system that prescribes a NAICS code identifier</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>NAICS Coding System x implies x is an Information Content Entity and x prescribes some Code Identifier y that designates a Business Function b</iof-av:semiFormalNaturalLanguageDefinition>
    </owl:Class>



    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ObservingProcess -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ObservingProcess">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/PlannedProcess"/>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000057"/>
                                <owl:someValuesFrom>
                                    <owl:Class>
                                        <owl:intersectionOf rdf:parseType="Collection">
                                            <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000040"/>
                                            <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/Agent"/>
                                        </owl:intersectionOf>
                                    </owl:Class>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/hasOutput"/>
                                <owl:someValuesFrom rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/InformationContentEntity"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:label>observing process</rdfs:label>
        <skos:editorialNote>this is based on the tentative definition provided in MTConnect ontology</skos:editorialNote>
        <iof-av:naturalLanguageDefinition>planned process in which an agent perceives or detects a material entity and selects it as worthy of observation, and which has as output some information content entity about the selected material entity</iof-av:naturalLanguageDefinition>
        <iof-av:naturalLanguageDefinition>process in which an agent perceives some aspect of a material entity</iof-av:naturalLanguageDefinition>
    </owl:Class>



    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/Organism -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/Organism">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000030"/>
        <rdfs:label>organism</rdfs:label>
        <iof-av:naturalLanguageDefinition>object made up of one or more cells and able to carry on the activities of life</iof-av:naturalLanguageDefinition>
    </owl:Class>



    <!-- https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/OrganizationCapability -->

    <owl:Class rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/OrganizationCapability">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/Capability"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/capabilityOf"/>
