                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/BusinessOrganization"/>
                                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/Person"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/agentIn"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/BuyingBusinessProcess"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000057"/>
                                        <owl:someValuesFrom>
                                            <owl:Class>
                                                <owl:intersectionOf rdf:parseType="Collection">
                                                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/MaterialProduct"/>
                                                    <owl:Class>
                                                        <owl:unionOf rdf:parseType="Collection">
                                                            <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/Manufacturer"/>
                                                            <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/Wholesaler"/>
                                                        </owl:unionOf>
                                                    </owl:Class>
                                                </owl:intersectionOf>
                                            </owl:Class>
                                        </owl:someValuesFrom>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/roleOf"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/BusinessOrganization"/>
                                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/Person"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/agentIn"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/SellingBusinessProcess"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000057"/>
                                        <owl:someValuesFrom rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/MaterialProduct"/>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/roleOf"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/BusinessOrganization"/>
                                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/Person"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/agentIn"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/SupplyingBusinessProcess"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000057"/>
                                        <owl:someValuesFrom>
                                            <owl:Class>
                                                <owl:intersectionOf rdf:parseType="Collection">
                                                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/MaterialProduct"/>
                                                    <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/customer"/>
                                                </owl:intersectionOf>
                                            </owl:Class>
                                        </owl:someValuesFrom>
