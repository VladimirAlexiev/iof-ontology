        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/Facility"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000115"/>
                        <owl:someValuesFrom rdf:resource="https://www.industrialontologies.org/ontology/supplychain/SupplyChainReferenceOntology/ProductionSystem"/>
