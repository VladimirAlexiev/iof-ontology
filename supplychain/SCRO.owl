    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#ActOfService -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#ActOfService">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.industrialontologies.org/core#PlannedProcess"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology#is_prescribed_by"/>
                        <owl:someValuesFrom rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology#ServiceSpecification"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <CommonCoreOntologies:definition>A planned process that realizes the concretization of a service plan specification.</CommonCoreOntologies:definition>
        <rdfs:label>service</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#Airway -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#Airway">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000006"/>
        <CommonCoreOntologies:definition>A spatia region that connects one specified location to another at a specified altitude, along which an aircraft that meets the requirements of the airway may be flown.</CommonCoreOntologies:definition>
        <rdfs:label>airway</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#Cargo -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#Cargo">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000040"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/bearer_of"/>
                        <owl:someValuesFrom rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology#CargoRole"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000040"/>
        <CommonCoreOntologies:definition>A material entity that bears a cargo role.</CommonCoreOntologies:definition>
        <rdfs:label>cargo</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#CargoRole -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#CargoRole">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000023"/>
        <CommonCoreOntologies:definition>A role borne by a material entity when transported by a transport equipment.</CommonCoreOntologies:definition>
        <rdfs:label>cargo role</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#CastingService -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#CastingService">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology#ManufacturingService"/>
        <CommonCoreOntologies:definition>A Manufacturing Service which includes at least one casting process as process part.</CommonCoreOntologies:definition>
        <rdfs:label>casting service</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#MachiningService -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#MachiningService">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology#ManufacturingService"/>
        <CommonCoreOntologies:definition>A Manufacturing Service includes at least one machining process as proess part.</CommonCoreOntologies:definition>
        <rdfs:label>machining service</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#ManufacturingProcess -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#ManufacturingProcess">
        <rdfs:subClassOf rdf:resource="http://www.industrialontologies.org/core#PlannedProcess"/>
        <CommonCoreOntologies:definition>A Planned Process in which some Material Artifact or Portion of Material is a specified input of the process, and in which some Material Artifact or Portion of Material is a specified output of the process, or in which a specified Material Artifact is modified.</CommonCoreOntologies:definition>
        <rdfs:label>manufacturing process</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#ManufacturingService -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#ManufacturingService">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#ActOfService"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology#has_occurrent_part"/>
                        <owl:someValuesFrom rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology#ManufacturingProcess"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology#ActOfService"/>
        <CommonCoreOntologies:definition>A Service which includes at least one manufacturing process as process part.</CommonCoreOntologies:definition>
        <rdfs:label>manufacturing service</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#Seaway -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#Seaway">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000006"/>
        <rdfs:label>seaway</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#ServiceConsumerRole -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#ServiceConsumerRole">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology/CustomerRole"/>
        <CommonCoreOntologies:definition>A role which inheres in a person or organization who uses a service</CommonCoreOntologies:definition>
        <rdfs:label>service consumer role</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#ServiceSpecification -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#ServiceSpecification">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/DirectiveInformationContentEntity"/>
        <CommonCoreOntologies:definition>.A Directive Information Content entity with service action specifications and service objective specifications as parts that, when concretized, is realized in a process comprised of production and consumption of an economic good such that production and consumption coincide.</CommonCoreOntologies:definition>
        <rdfs:label>service plan specification</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#Supply_Chain_Product_Production_Process_Aggregate -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#Supply_Chain_Product_Production_Process_Aggregate">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology/ProductProductionProcess"/>
        <CommonCoreOntologies:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Product Production Process Aggregate than involves more than one agent as participant.</CommonCoreOntologies:definition>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">supply chain product production process aggregate</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology#TransportationService -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology#TransportationService">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology#ActOfService"/>
        <rdfs:label>transportation service</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology/ActOfTransportation -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology/ActOfTransportation">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/ActOfLocationChange"/>
        <CommonCoreOntologies:alternative_label>act of shipping</CommonCoreOntologies:alternative_label>
        <CommonCoreOntologies:definition>An Act of Location Change involving the movement of material entities through some Transportation Agent.</CommonCoreOntologies:definition>
        <rdfs:label>act of transportation</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology/ActofPositionChange -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology/ActofPositionChange">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/ActOfMotion"/>
        <CommonCoreOntologies:definition>An Act of Motion in which the position (i.e. the orientation, alignment, or arrangement) of some Object or of one or more of an Object&apos;s parts is changed by some Agent.</CommonCoreOntologies:definition>
        <rdfs:comment>An Act of Position Change does not entail a change of location.</rdfs:comment>
        <rdfs:label>Act of Position Change</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology/ActofSupplierEvaluation -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology/ActofSupplierEvaluation">
        <rdfs:subClassOf rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/ActOfAppraisal"/>
        <CommonCoreOntologies:definition>An Act of Measuring that involves evaluating, assessing, estimating, or judging the capabilities of an Agent that is intended to become a member of a Supply Chain.</CommonCoreOntologies:definition>
        <rdfs:label>act of supplier evaluation</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology/AircraftManufacturingFactory -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology/AircraftManufacturingFactory">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology/Factory"/>
        <CommonCoreOntologies:definition>A Facility that is designed for manufacturing aircraft.</CommonCoreOntologies:definition>
        <rdfs:label>aircraft manufacturing factory</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology/AssemblyServiceProviderRole -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology/AssemblyServiceProviderRole">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology/ServiceProviderRole"/>
        <CommonCoreOntologies:definition>A Service Provider Role that is borne by an Agent to provide an Assembly Service to some Agents in some Supply Chain.</CommonCoreOntologies:definition>
        <rdfs:label>assembly service provider role</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology/Batch -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology/Batch">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology/TraceableResourceUnit"/>
        <CommonCoreOntologies:definition>A Traceable Resource Unit that is the output of a batch process</CommonCoreOntologies:definition>
        <rdfs:label>batch</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology/CastingServiceProviderRole -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology/CastingServiceProviderRole">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology/ManufacturingServiceProviderRole"/>
        <CommonCoreOntologies:definition>A Manufacturing Service Provider Role that is borne by an Agent to provide a Casting Service to some Agents in some Supply Chain.</CommonCoreOntologies:definition>
        <rdfs:label>casting service provider role</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology/CastingSupplier -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology/CastingSupplier">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology/ManufacturingSupplier"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/bearer_of"/>
                        <owl:someValuesFrom rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology/CastingServiceProviderRole"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology/ManufacturingSupplier"/>
        <CommonCoreOntologies:definition>A Manufacturing Supplier that bears a Casting Service Provider Role.</CommonCoreOntologies:definition>
        <rdfs:label>casting supplier</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology/ComponentProviderRole -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology/ComponentProviderRole">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology/MaterialProviderRole"/>
        <CommonCoreOntologies:definition>A Supplier Role that is borne by an Agent to sell some Component to some other Agents.</CommonCoreOntologies:definition>
        <rdfs:label>component provider role</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology/CustomerRole -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology/CustomerRole">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000023"/>
        <CommonCoreOntologies:definition>A Role that is borne by an Agent to buy or consume a Product or Service.</CommonCoreOntologies:definition>
        <rdfs:label>customer role</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology/Facility -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology/Facility">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology/MaterialArtifact"/>
        <CommonCoreOntologies:definition>An Artifact that is designed as a building or campus dedicated to some specific purpose.</CommonCoreOntologies:definition>
        <rdfs:label>facility</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology/Factory -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology/Factory">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology/Facility"/>
        <CommonCoreOntologies:definition>A Facility that is designed for manufacturing or refining discrete or continuous material products.</CommonCoreOntologies:definition>
        <rdfs:label>factory</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology/ForgingServiceProviderRole -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology/ForgingServiceProviderRole">
        <rdfs:subClassOf rdf:resource="http://infoneer.txstate.edu/IOF/SupplyChainOntology/ManufacturingServiceProviderRole"/>
        <CommonCoreOntologies:definition>A Manufacturing Service Provider Role that is borne by an Agent to provide a Forging Service to some Agents in some Supply Chain.</CommonCoreOntologies:definition>
        <rdfs:label>forging service provider role</rdfs:label>
    </owl:Class>
    


    <!-- http://infoneer.txstate.edu/IOF/SupplyChainOntology/ForgingSupplier -->

    <owl:Class rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology/ForgingSupplier">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://infoneer.txstate.edu/IOF/SupplyChainOntology/ManufacturingSupplier"/>
