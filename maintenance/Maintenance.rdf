    <!-- https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/DegradedState -->

    <owl:Class rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/DegradedState">
        <rdfs:subClassOf rdf:resource="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/MaintenanceState" />
        <owl:disjointWith rdf:resource="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/FailedState" />
        <owl:disjointWith rdf:resource="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/OperatingState" />
        <rdfs:comment>natural language definition from EN:13306</rdfs:comment>
        <rdfs:label>degraded state</rdfs:label>
        <iof-av:naturalLanguageDefinition>state of reduced ability to perform as required but with acceptable reduced performance</iof-av:naturalLanguageDefinition>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</iof-av:isPrimitive>
    </owl:Class>


    <!-- https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/DispositionToFail -->

    <owl:Class rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/DispositionToFail">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/UndesireableDisposition" />
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000054" />
                        <owl:someValuesFrom rdf:resource="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/FailureProcess" />
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/UndesireableDisposition" />
        <rdfs:label>disposition to fail</rdfs:label>
        <iof-av:firstOrderLogicDefinition>UndesireableDisposition(x) ∧ ∃y(FailureProcess(y) ∧ hasRealization(x,y)) ↔ DispositionToFail(x)</iof-av:firstOrderLogicDefinition>
        <iof-av:naturalLanguageDefinition>undesirable disposition of an item causing it to fail as a result of a failure process</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>&apos;disposition to fail&apos;: every instance of &apos;disposition to fail&apos; is defined as exactly an instance of &apos;undesirable disposition&apos; that &apos;has realization&apos; some &apos;failure process&apos;</iof-av:semiFormalNaturalLanguageDefinition>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</iof-av:isPrimitive>
    </owl:Class>


    <!-- https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/FailedState -->

    <owl:Class rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/FailedState">
        <rdfs:subClassOf rdf:resource="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/MaintenanceState" />
        <owl:disjointWith rdf:resource="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/OperatingState" />
        <rdfs:comment>This is the definition used in the paper Woods C, Selway M, Hodkiewicz M, Ameri F, Stumptner M, Sobel W. On the Notion of Maintenance State for Industrial Assets. InJOWO 2021.</rdfs:comment>
        <rdfs:label>failed state</rdfs:label>
        <iof-av:firstOrderLogicDefinition>to do</iof-av:firstOrderLogicDefinition>
        <iof-av:naturalLanguageDefinition>state of an item being unable to perform a required function due to a failure event</iof-av:naturalLanguageDefinition>
        <iof-av:semi-formalNaturalLanguageDefinition>to do</iof-av:semi-formalNaturalLanguageDefinition>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</iof-av:isPrimitive>
    </owl:Class>


    <!-- https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/FailureEffect -->

    <owl:Class rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/FailureEffect">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015" />
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000062" />
                <owl:someValuesFrom rdf:resource="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/FailureEvent" />
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>natural language definition from IEC 60812:2020</rdfs:comment>
        <rdfs:label>failure effect</rdfs:label>
        <iof-av:naturalLanguageDefinition>consequence of failure, within or beyond the boundary of the failed item</iof-av:naturalLanguageDefinition>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</iof-av:isPrimitive>
        <iof-av:primitiveRationale></iof-av:primitiveRationale>
    </owl:Class>


    <!-- https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/FailureEvent -->

    <owl:Class rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/FailureEvent">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000057" />
                <owl:someValuesFrom rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/MaintainableMaterialItem" />
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000063" />
                <owl:someValuesFrom rdf:resource="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/FailureEffect" />
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/Event" />
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000063" />
                <owl:someValuesFrom rdf:resource="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/FailedState" />
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>failure event</rdfs:label>
        <skos:altLabel>failure</skos:altLabel>
        <iof-av:directSource xml:lang="en">natural language definition from EN13306, 2017</iof-av:directSource>
        <iof-av:naturalLanguageDefinition xml:lang="en">loss of ability of an item to perform a required function</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition xml:lang="en">&apos;failure event&apos; : every instance of &apos;failure event&apos; is defined as an instance of &apos;event&apos; where some process which realizes the &apos;primary function&apos; of a &apos;maintainable item&apos; ceases</iof-av:semiFormalNaturalLanguageDefinition>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</iof-av:isPrimitive>
    </owl:Class>


    <!-- https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/FailureModeObservation -->

    <owl:Class rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/FailureModeObservation">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/DescriptiveInformationContentEntity" />
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/isOutputOf" />
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000015" />
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000057" />
                                        <owl:someValuesFrom>
                                            <owl:Class>
                                                <owl:intersectionOf rdf:parseType="Collection">
                                                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/Person" />
                                                    <owl:Restriction>
                                                        <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/observesAtSomeTime" />
                                                        <owl:someValuesFrom rdf:resource="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/FailedState" />
                                                    </owl:Restriction>
                                                </owl:intersectionOf>
                                            </owl:Class>
                                        </owl:someValuesFrom>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/DescriptiveInformationContentEntity" />
        <rdfs:comment>natural language definition from EN:13306</rdfs:comment>
        <rdfs:label>failure mode observation</rdfs:label>
        <iof-av:firstOrderLogicDefinition>DescriptiveInformationContentEntity(x) ∧ ∃y,z,s(Process(y) ^  outputOf(x,y) ^ Person(z) ^ hasParticipantAtSomeTime(y,z) ∧ FailedState(s) ∧ observesAtSomeTime(z, s) ↔ FailureModeObservation(x)</iof-av:firstOrderLogicDefinition>
        <iof-av:naturalLanguageDefinition>observation of the manner in which the inability of an item to perform a required function occurs</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>&apos;failure mode observation&apos;: every instance of &apos;failure mode observation&apos; is defined as an &apos;information content entity&apos; recorded by a &apos;person&apos; on observing the &apos;failed state&apos; of an item</iof-av:semiFormalNaturalLanguageDefinition>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</iof-av:isPrimitive>
    </owl:Class>


    <!-- https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/FailureProcess -->

    <owl:Class rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/FailureProcess">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000055" />
                <owl:someValuesFrom rdf:resource="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/DispositionToFail" />
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015" />
        <rdfs:label>failure process</rdfs:label>
        <skos:altLabel>functional failure</skos:altLabel>
        <iof-av:firstOrderLogicDefinition>Process(x) ∧ ∃y(DispositionToFail(y) ∧ realizes(x,y)) ↔ FailureProcess(x)</iof-av:firstOrderLogicDefinition>
        <iof-av:naturalLanguageDefinition xml:lang="en">process that changes some quality of an item causing the the item to become degraded or failed</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>&apos;failure process&apos;: every instance of &apos;failure process&apos; is defined as exactly an instance of &apos;process&apos; that &apos;realizes&apos; some &apos;disposition to fail&apos;</iof-av:semiFormalNaturalLanguageDefinition>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</iof-av:isPrimitive>
    </owl:Class>


    <!-- https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/FunctioningProcess -->

    <owl:Class rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/FunctioningProcess">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000055" />
                <owl:someValuesFrom rdf:resource="http://purl.obolibrary.org/obo/BFO_0000034" />
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015" />
        <rdfs:label>functioning process</rdfs:label>
        <iof-av:firstOrderLogicDefinition>Process(x) ∧ ∃y(Function(y) ∧ realizes(x,y)) ↔ FunctioningProcess(x)</iof-av:firstOrderLogicDefinition>
        <iof-av:naturalLanguageDefinition xml:lang="en">process that enables an item to perform a function</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>&apos;functioning process&apos;: every instance of &apos;functioning process&apos; is defined as exactly an instance of &apos;process&apos; that &apos;realizes&apos; some &apos;function&apos;</iof-av:semiFormalNaturalLanguageDefinition>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</iof-av:isPrimitive>
    </owl:Class>


    <!-- https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/MaintenanceActivityProcess -->

    <owl:Class rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/MaintenanceActivityProcess">
        <rdfs:subClassOf rdf:resource="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/MaintenanceProcess" />
        <rdfs:comment>In maintenance, we consider the maintenance activity to be the most atomic unit of maintenance processes.</rdfs:comment>
        <rdfs:label>maintenance activity</rdfs:label>
        <iof-av:naturalLanguageDefinition>single action to retain or restore the function of a maintainable item</iof-av:naturalLanguageDefinition>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</iof-av:isPrimitive>
        <iof-av:primitiveRationale>at this stage we do not have sufficient constructs to create necessary and sufficent conditions for this class.</iof-av:primitiveRationale>
    </owl:Class>


    <!-- https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/MaintenanceProcess -->

    <owl:Class rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/MaintenanceProcess">
        <rdfs:subClassOf rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/PlannedProcess" />
        <rdfs:label>maintenance process</rdfs:label>
        <iof-av:naturalLanguageDefinition>process to do with retaining or restoring the function of a maintainable item</iof-av:naturalLanguageDefinition>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</iof-av:isPrimitive>
        <iof-av:primitiveRationale>maintenance process is used in the definition of maintainable item role</iof-av:primitiveRationale>
    </owl:Class>


    <!-- https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/MaintenanceState -->

    <owl:Class rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/MaintenanceState">
        <rdfs:subClassOf rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/State" />
        <rdfs:comment>This is the definition used in the paper Woods C, Selway M, Hodkiewicz M, Ameri F, Stumptner M, Sobel W. On the Notion of Maintenance State for Industrial Assets. InJOWO 2021.</rdfs:comment>
        <rdfs:label>maintenance state</rdfs:label>
        <iof-av:naturalLanguageDefinition xml:lang="en">stasis that holds during a temporal interval when the realizable functions and capabilities of the participating item, or the grade of realization of those functions and capabilities, remain unchanged</iof-av:naturalLanguageDefinition>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</iof-av:isPrimitive>
        <iof-av:primitiveRationale>We do not have the terms to deal with the concepts in the 2nd half of the NL definition about &apos;grade of realization&apos;</iof-av:primitiveRationale>
    </owl:Class>


    <!-- https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/OperatingState -->

    <owl:Class rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/OperatingState">
        <rdfs:subClassOf rdf:resource="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/MaintenanceState" />
        <rdfs:comment>This is the definition used in the paper Woods C, Selway M, Hodkiewicz M, Ameri F, Stumptner M, Sobel W. On the Notion of Maintenance State for Industrial Assets. InJOWO 2021.</rdfs:comment>
        <rdfs:label>operating state</rdfs:label>
        <iof-av:naturalLanguageDefinition>state of an item being able to perform a required function assuming that the external resources, if required, are provided</iof-av:naturalLanguageDefinition>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</iof-av:isPrimitive>
    </owl:Class>


    <!-- https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/PrimaryFunction -->

    <owl:Class rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/PrimaryFunction">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000034" />
        <rdfs:comment>The natural language definition comes from EN:13306</rdfs:comment>
        <rdfs:label>required function</rdfs:label>
        <skos:altLabel>primary function</skos:altLabel>
        <iof-av:firstOrderLogicDefinition>Function(x) ∧ ∃y(MaintainableMaterialItem(y) ∧ FunctioningProcess(z) ∧ participatesInAtSomeTime(y,z)) ∧ functionOf(x,y) ↔ RequiredFunction(x))</iof-av:firstOrderLogicDefinition>
        <iof-av:naturalLanguageDefinition>function, combination of functions, or a total combination of functions of an item which are considered necessary to fulfil a given requirement</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>&apos;required function&apos;: every instance of ‘required function’ is defined as an instance of ‘function’ that is the &apos;function of&apos; some &apos;maintainable material item&apos; that &apos;participates in at some time&apos; some &apos;functioning process&apos;</iof-av:semiFormalNaturalLanguageDefinition>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</iof-av:isPrimitive>
        <iof-av:primitiveRationale>Using the current terms, we cannot differentiate between a primary and a secondary function.</iof-av:primitiveRationale>
    </owl:Class>


    <!-- https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/Qualification -->

    <owl:Class rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/Qualification">
        <rdfs:subClassOf rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/RequirementSpecification" />
        <rdfs:comment>To be considered in IOF core in v2 of core</rdfs:comment>
        <rdfs:label>qualification</rdfs:label>
        <iof-av:naturalLanguageDefinition>assessed skill for a specific task</iof-av:naturalLanguageDefinition>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</iof-av:isPrimitive>
    </owl:Class>


    <!-- https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/QualifiedPerson -->

    <owl:Class rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/QualifiedPerson">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/Person" />
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000056" />
                        <owl:someValuesFrom rdf:resource="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/MaintenanceActivityProcess" />
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/satisfies" />
                        <owl:someValuesFrom rdf:resource="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/Qualification" />
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/Person" />
        <rdfs:label>Qualified Person</rdfs:label>
        <iof-av:firstOrderLogicDefinition>Person(x) ∧ ∃y(MaintenanceActivity(y) ∧ partcipatesInAtSomeTime(x,y))  ∧ ∃z(Qualification(z) ∧ satisfies(x,z))  ↔ QualifiedPerson(x)</iof-av:firstOrderLogicDefinition>
        <iof-av:naturalLanguageDefinition>A person qualified to perform a specified process or task</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>&apos;qualified person&apos;: every  instance of ‘qualified person’ is defined as exactly an instance of ‘person’ that ‘participates in at some time’ some ‘maintenance activity’ and &apos;satifies&apos; some &apos;qualification&apos;</iof-av:semiFormalNaturalLanguageDefinition>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</iof-av:isPrimitive>
    </owl:Class>


    <!-- https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/SupportingActivity -->

    <owl:Class rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/SupportingActivity">
        <rdfs:subClassOf rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/PlannedProcess" />
        <rdfs:label>supporting maintenance activity</rdfs:label>
        <iof-av:naturalLanguageDefinition>single action in support of the execution of a maintenance process</iof-av:naturalLanguageDefinition>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</iof-av:isPrimitive>
        <iof-av:primitiveRationale>at this stage we do not have sufficient constructs to create necessary and sufficent conditions for this class.</iof-av:primitiveRationale>
    </owl:Class>


    <!-- https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/UndesireableDisposition -->

    <owl:Class rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/UndesireableDisposition">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000016" />
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000054" />
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/DegradedState" />
                                    <rdf:Description rdf:about="https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/FailedState" />
                                </owl:unionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016" />
        <rdfs:label>undesireable disposition</rdfs:label>
        <iof-av:firstOrderLogicDefinition>Disposition(x) ∧ ∃y,z((DegradedState(y) ∨ FailedState(z))  ∧ (hasRealization(x,y) ∨ hasRealization(x,z)) ↔ UndesireableDisposition(x)</iof-av:firstOrderLogicDefinition>
        <iof-av:naturalLanguageDefinition>disposition of an item to fail to perform a required function</iof-av:naturalLanguageDefinition>
        <iof-av:semiFormalNaturalLanguageDefinition>&apos;undesireable disposition&apos;: every  instance of ‘undesireable disposition’ is defined as exactly an instance of ‘disposition’ that ‘has realization’ some ‘failed state’ or &apos;operating state&apos;</iof-av:semiFormalNaturalLanguageDefinition>
        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</iof-av:isPrimitive>
    </owl:Class>
