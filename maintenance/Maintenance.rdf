        <iof-av:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</iof-av:isPrimitive>
    </owl:Class>


    <!-- https://dev.industrialontologies.org/ontology/maintenance/MaintenanceReferenceOntology/RequiredFunction -->

